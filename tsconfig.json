{
    "compilerOptions": {
     "outDir": "./dist/",
     "sourceMap": true,
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "downlevelIteration": true,
      "lib":["dom","ESNext","ES2015"],
    },
    "exclude": [
      "node_modules",
      "dist",
    ],
  }